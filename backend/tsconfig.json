{
  "compilerOptions": {
    "target": "ESNext",
    "module": "esnext",
    "declaration": true,
    "outDir": "out",
    "declarationDir": "out",
    "emitDeclarationOnly": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
