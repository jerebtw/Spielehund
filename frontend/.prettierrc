{
	"useTabs": true,
	"singleQuote": false,
	"bracketSameLine": true,
	"endOfLine": "lf",
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
